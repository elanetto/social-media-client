{
  "name": "css-frameworks-ca",
  "version": "1.0.0",
  "description": "Test",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "prepare": "husky install",
    "test:cypress": "cypress run",
    "test:jest": "jest"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.22",
    "@babel/preset-env": "7.22",
    "cypress": "^13.15.0",
    "eslint": "^9.13.0",
    "eslint-plugin-cypress": "^4.0.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "prettier": "^3.3.3",
    "sass": "^1.54.8",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:cypress && npm run test:jest",
      "pre-commit": "npm run lint && npm run test:jest",
      "post-merge": "npm run test:cypress",
      "post-commit": "echo 'Commit completed successfully!'"
    }
  }
}
